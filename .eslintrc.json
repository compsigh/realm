{
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "standard"
  ],
  "rules": {
    "brace-style": [
      "error",
      "stroustrup"
    ],
    "curly": [
      "error",
      "multi"
    ],
    "camelcase": [
      "error",
      {
        "ignoreImports": true,
        "ignoreDestructuring": true,
        "allow": ["max_tokens"]
      }
    ]
  }
}
